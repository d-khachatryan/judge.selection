@using  stvsystem.Data
@model SettingItem


@using (Html.BeginForm("StartSelection", "Setting", FormMethod.Post, new { id = "frmInitialize" }))
{
    @Html.HiddenFor(model => model.SettingID)
    @Html.HiddenFor(model => model.SelectionStatus)

    <div class="container-maxwidth500">
        <div class="card">
            <div class="card-header">
                Ցանկանում եք սկսե՞լ քվեարկությունը
            </div>
            <div class="card-footer">
                <button type="submit" id="SubmitButton" class="btn btn-sm btn-success"><i class="icons icon-arrow-right"></i>&nbsp;Սկսել</button>
                <button type="button" class="btn btn-sm btn-secondary" onclick="location.href='@Url.Action("Index", "Setting")'"><i class="icons icon-close"></i>&nbsp;Չեղարկել</button>
            </div>
        </div>
    </div>
}

